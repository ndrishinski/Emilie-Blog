
<div class="container">
  <ul class="nav nav-tabs">
  
    <ul><%= link_to 'blog', blogs_path%></ul>
    <ul><%= link_to 'portfolio', portfolio_path%> </ul>
    <ul><%= link_to 'contact', contact_path%></ul>
    <ul><%= link_to 'about', about_path%></ul>

  </ul>
</div>
<p id="notice"><%= notice %></p>
  <div class="weiner"
    <h1>Emilie's Sexy Blogs</h1>

  </div>
    <table>
      <thead>
        <tr>
          <th>Title</th>
          <th>Content</th>
          <th colspan="3"></th>
        </tr>
      </thead>

  
      <tbody>
        <% @blogs.each do |blog| %>
          <tr>
            <td><%= blog.title %></td>
            <td><%= blog.content %></td>
            <td><%= link_to 'Show', blog %></td>
            <td><%= link_to 'Edit', edit_blog_path(blog) %></td>
            <td><%= link_to 'Delete', blog, method: :delete, data: { confirm: 'You sure about that homie?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
<br>

<%= link_to 'New Blog', new_blog_path %>
